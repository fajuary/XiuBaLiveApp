<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/MatchWarp"
    >

    <TextView
        android:id="@+id/reycleview_kbinfo_getTypeNameTxt"
        android:paddingLeft="15dp"
        android:paddingTop="15dp"
        style="@style/WarpWarp"
        android:textColor="#000000"
        android:textSize="16sp"
        android:text="每日登录"

        />

    <TextView
        android:id="@+id/reycleview_kbinfo_finishTimeTxt"
        style="@style/WarpWarp"
        android:layout_below="@+id/reycleview_kbinfo_getTypeNameTxt"
        android:layout_marginTop="5dp"
        android:paddingLeft="15dp"
        android:textColor="#A0A1AA"
        android:textSize="12sp"
        android:text="2018.06.04"

        />

    <TextView
        android:id="@+id/reycleview_kbinfo_getloseMoneyTxt"

        style="@style/WarpWarp"
        android:paddingRight="15dp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="14dp"
        android:textColor="#FF6262"
        android:textSize="18sp"
        android:text="- 0.00"
        />
    <View
        android:layout_below="@+id/reycleview_kbinfo_finishTimeTxt"
        android:layout_marginTop="15dp"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#F1F3FA"
        />
</RelativeLayout>
